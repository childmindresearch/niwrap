{
  "tool-version": "1.0.0",
  "name": "get_afni_model_PRF_6_BAD",
  "author": "AFNI Team",
  "description": "Command line tool for obtaining AFNI pRF model",
  "url": "https://afni.nimh.nih.gov/pub/dist/doc/program_help/get_afni_model_PRF_6_BAD.html",
  "command-line": "get_afni_model_PRF_6_BAD [AMPLITUDE] [X_COORD] [Y_COORD] [SIGMA] [SIGRAT] [THETA]",
  "container-image": {
    "image": "fcpindi/c-pac:latest",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Amplitude parameter A",
      "value-key": "[AMPLITUDE]",
      "type": "Number",
      "optional": false,
      "id": "amplitude",
      "name": "Amplitude"
    },
    {
      "description": "X coordinate parameter x",
      "value-key": "[X_COORD]",
      "type": "Number",
      "optional": false,
      "id": "x_coord",
      "name": "X Coordinate"
    },
    {
      "description": "Y coordinate parameter y",
      "value-key": "[Y_COORD]",
      "type": "Number",
      "optional": false,
      "id": "y_coord",
      "name": "Y Coordinate"
    },
    {
      "description": "Sigma parameter sigma",
      "value-key": "[SIGMA]",
      "type": "Number",
      "optional": false,
      "id": "sigma",
      "name": "Sigma"
    },
    {
      "description": "Sigma ratio parameter sigrat",
      "value-key": "[SIGRAT]",
      "type": "Number",
      "optional": false,
      "id": "sigrat",
      "name": "Sigma Ratio"
    },
    {
      "description": "Theta parameter theta (in radians)",
      "value-key": "[THETA]",
      "type": "Number",
      "optional": false,
      "id": "theta",
      "name": "Theta"
    }
  ],
  "schema-version": "0.5"
}
