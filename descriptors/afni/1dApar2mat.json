{
  "tool-version": "24.2.06",
  "name": "1dApar2mat",
  "author": "AFNI Developers",
  "description": "Computes the affine transformation matrix from the set of 3dAllineate parameters.",
  "command-line": "1dApar2mat [X_SHIFT] [Y_SHIFT] [Z_SHIFT] [Z_ANGLE] [X_ANGLE] [Y_ANGLE] [X_SCALE] [Y_SCALE] [Z_SCALE] [Y_X_SHEAR] [Z_X_SHEAR] [Z_Y_SHEAR]",
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  },
  "inputs": [
    {
      "description": "x-shift in mm",
      "value-key": "[X_SHIFT]",
      "type": "Number",
      "optional": false,
      "id": "x_shift",
      "name": "X Shift"
    },
    {
      "description": "y-shift in mm",
      "value-key": "[Y_SHIFT]",
      "type": "Number",
      "optional": false,
      "id": "y_shift",
      "name": "Y Shift"
    },
    {
      "description": "z-shift in mm",
      "value-key": "[Z_SHIFT]",
      "type": "Number",
      "optional": false,
      "id": "z_shift",
      "name": "Z Shift"
    },
    {
      "description": "z-angle (roll) in degrees",
      "value-key": "[Z_ANGLE]",
      "type": "Number",
      "optional": false,
      "id": "z_angle",
      "name": "Z Angle"
    },
    {
      "description": "x-angle (pitch) in degrees",
      "value-key": "[X_ANGLE]",
      "type": "Number",
      "optional": false,
      "id": "x_angle",
      "name": "X Angle"
    },
    {
      "description": "y-angle (yaw) in degrees",
      "value-key": "[Y_ANGLE]",
      "type": "Number",
      "optional": false,
      "id": "y_angle",
      "name": "Y Angle"
    },
    {
      "description": "x-scale factor in [0.10,10.0]",
      "value-key": "[X_SCALE]",
      "type": "Number",
      "maximum": 10.0,
      "minimum": 0.1,
      "optional": false,
      "id": "x_scale",
      "name": "X Scale"
    },
    {
      "description": "y-scale factor in [0.10,10.0]",
      "value-key": "[Y_SCALE]",
      "type": "Number",
      "maximum": 10.0,
      "minimum": 0.1,
      "optional": false,
      "id": "y_scale",
      "name": "Y Scale"
    },
    {
      "description": "z-scale factor in [0.10,10.0]",
      "value-key": "[Z_SCALE]",
      "type": "Number",
      "maximum": 10.0,
      "minimum": 0.1,
      "optional": false,
      "id": "z_scale",
      "name": "Z Scale"
    },
    {
      "description": "y/x-shear factor in [-0.3333,0.3333]",
      "value-key": "[Y_X_SHEAR]",
      "type": "Number",
      "maximum": 0.3333,
      "minimum": -0.3333,
      "optional": false,
      "id": "y_x_shear",
      "name": "Y/X Shear"
    },
    {
      "description": "z/x-shear factor in [-0.3333,0.3333]",
      "value-key": "[Z_X_SHEAR]",
      "type": "Number",
      "maximum": 0.3333,
      "minimum": -0.3333,
      "optional": false,
      "id": "z_x_shear",
      "name": "Z/X Shear"
    },
    {
      "description": "z/y-shear factor in [-0.3333,0.3333]",
      "value-key": "[Z_Y_SHEAR]",
      "type": "Number",
      "maximum": 0.3333,
      "minimum": -0.3333,
      "optional": false,
      "id": "z_y_shear",
      "name": "Z/Y Shear"
    }
  ],
  "schema-version": "0.5",
  "url": "https://afni.nimh.nih.gov/"
}