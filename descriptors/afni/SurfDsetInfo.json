{
  "tool-version": "24.2.06",
  "name": "SurfDsetInfo",
  "author": "AFNI Developers",
  "description": "Provides information about surface datasets (DSET).",
  "command-line": "SurfDsetInfo [INPUT_DSETS] [DEBUG_LEVEL] [NOVOLREG] [NOXFORM] [SETENV] [TRACE] [EXTREME_TRACE] [NOMALL] [YESMALL] [MINI_HELP] [HELP] [EXTREME_HELP] [HELP_VIEW] [HELP_WEB] [HELP_FIND] [HELP_RAW] [HELP_SPX] [HELP_ASPX] [ALL_OPTS]",
  "inputs": [
    {
      "description": "Input dataset",
      "value-key": "[INPUT_DSETS]",
      "type": "File",
      "list": true,
      "optional": false,
      "id": "input_dsets",
      "name": "Input Datasets",
      "command-line-flag": "-input"
    },
    {
      "description": "Debug level. If DBG = 2, show full dataset information in NIML form.",
      "command-line-flag": "-debug",
      "value-key": "[DEBUG_LEVEL]",
      "type": "Number",
      "optional": true,
      "id": "debug_level",
      "name": "Debug Level",
      "integer": true,
      "minimum": 0
    },
    {
      "description": "Ignore any Rotate, Volreg, Tagalign, or WarpDrive transformations",
      "command-line-flag": "-novolreg",
      "value-key": "[NOVOLREG]",
      "type": "Flag",
      "optional": true,
      "id": "novolreg",
      "name": "Ignore Volume Registration Transformations"
    },
    {
      "description": "Same as -novolreg",
      "command-line-flag": "-noxform",
      "value-key": "[NOXFORM]",
      "type": "Flag",
      "optional": true,
      "id": "noxform",
      "name": "No Transformations"
    },
    {
      "description": "Set environment variable",
      "command-line-flag": "-setenv",
      "value-key": "[SETENV]",
      "type": "String",
      "optional": true,
      "id": "setenv",
      "name": "Set Environment Variable"
    },
    {
      "description": "Turns on In/Out debug and Memory tracing",
      "command-line-flag": "-trace",
      "value-key": "[TRACE]",
      "type": "Flag",
      "optional": true,
      "id": "trace",
      "name": "IO and Memory Tracing"
    },
    {
      "description": "Turns on extreme tracing",
      "command-line-flag": "-TRACE",
      "value-key": "[EXTREME_TRACE]",
      "type": "Flag",
      "optional": true,
      "id": "extreme_trace",
      "name": "Extreme Tracing"
    },
    {
      "description": "Turn off memory tracing",
      "command-line-flag": "-nomall",
      "value-key": "[NOMALL]",
      "type": "Flag",
      "optional": true,
      "id": "nomall",
      "name": "Turn Off Memory Tracing"
    },
    {
      "description": "Turn on memory tracing (default)",
      "command-line-flag": "-yesmall",
      "value-key": "[YESMALL]",
      "type": "Flag",
      "optional": true,
      "id": "yesmall",
      "name": "Turn On Memory Tracing"
    },
    {
      "description": "Mini help",
      "command-line-flag": "-h",
      "value-key": "[MINI_HELP]",
      "type": "Flag",
      "optional": true,
      "id": "mini_help",
      "name": "Mini Help"
    },
    {
      "description": "Show entire help output",
      "command-line-flag": "-help",
      "value-key": "[HELP]",
      "type": "Flag",
      "optional": true,
      "id": "help",
      "name": "Help"
    },
    {
      "description": "Show extreme help",
      "command-line-flag": "-HELP",
      "value-key": "[EXTREME_HELP]",
      "type": "Flag",
      "optional": true,
      "id": "extreme_help",
      "name": "Extreme Help"
    },
    {
      "description": "Open help in text editor",
      "command-line-flag": "-h_view",
      "value-key": "[HELP_VIEW]",
      "type": "Flag",
      "optional": true,
      "id": "help_view",
      "name": "Help View"
    },
    {
      "description": "Open help in web browser",
      "command-line-flag": "-h_web",
      "value-key": "[HELP_WEB]",
      "type": "Flag",
      "optional": true,
      "id": "help_web",
      "name": "Help Web"
    },
    {
      "description": "Look for lines in help output that match the specified word",
      "command-line-flag": "-h_find",
      "value-key": "[HELP_FIND]",
      "type": "String",
      "optional": true,
      "id": "help_find",
      "name": "Help Find Word"
    },
    {
      "description": "Show unedited help string",
      "command-line-flag": "-h_raw",
      "value-key": "[HELP_RAW]",
      "type": "Flag",
      "optional": true,
      "id": "help_raw",
      "name": "Help Raw"
    },
    {
      "description": "Show help string in sphinx format, but do not autoformat",
      "command-line-flag": "-h_spx",
      "value-key": "[HELP_SPX]",
      "type": "Flag",
      "optional": true,
      "id": "help_spx",
      "name": "Help SPX"
    },
    {
      "description": "Show help string in sphinx with autoformatting",
      "command-line-flag": "-h_aspx",
      "value-key": "[HELP_ASPX]",
      "type": "Flag",
      "optional": true,
      "id": "help_aspx",
      "name": "Help ASPX"
    },
    {
      "description": "Attempt to identify all options for the program from the help output",
      "command-line-flag": "-all_opts",
      "value-key": "[ALL_OPTS]",
      "type": "Flag",
      "optional": true,
      "id": "all_opts",
      "name": "All Options"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  },
  "url": "https://afni.nimh.nih.gov/"
}
