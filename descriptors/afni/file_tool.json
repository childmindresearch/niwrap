{
  "tool-version": "24.2.06",
  "name": "file_tool",
  "author": "AFNI Developers",
  "description": "Program to display or modify sections of a file",
  "url": "https://afni.nimh.nih.gov/",
  "command-line": "file_tool [HELP] [VERSION] [HIST] [DEBUG] [INFILES] [GE_ALL] [GE_HEADER] [GE_EXTRAS] [GE_UV17] [GE_RUN] [GE_OFF] [GE4_ALL] [GE4_IMAGE] [GE4_SERIES] [GE4_STUDY] [DEF_ANA_HDR] [DIFF_ANA_HDRS] [DISP_ANA_HDR] [HEX] [MOD_ANA_HDR] [MOD_FIELD] [PREFIX] [OVERWRITE] [SHOW_BAD_ALL] [SHOW_BAD_BACKSLASH] [SHOW_BAD_CHAR] [SHOW_FILE_TYPE] [FIX_RICH_QUOTES] [TEST] [LENGTH] [MOD_DATA] [MOD_TYPE] [OFFSET] [QUIET] [DISP_HEX] [DISP_HEX1] [DISP_HEX2] [DISP_HEX4] [DISP_INT2] [DISP_INT4] [DISP_REAL4] [SWAP_BYTES]",
  "schema-version": "0.5",
  "inputs": [
    {
      "id": "help",
      "name": "Help",
      "description": "Show help information",
      "command-line-flag": "-help",
      "type": "Flag",
      "optional": true,
      "value-key": "[HELP]"
    },
    {
      "id": "version",
      "name": "Version",
      "description": "Show version information",
      "command-line-flag": "-version",
      "type": "Flag",
      "optional": true,
      "value-key": "[VERSION]"
    },
    {
      "id": "hist",
      "name": "Modification History",
      "description": "Show the program's modification history",
      "command-line-flag": "-hist",
      "type": "Flag",
      "optional": true,
      "value-key": "[HIST]"
    },
    {
      "id": "debug",
      "name": "Debug Level",
      "description": "Print extra info along the way, default is 0, max is 2",
      "command-line-flag": "-debug",
      "value-key": "[DEBUG]",
      "type": "Number",
      "optional": true
    },
    {
      "id": "infiles",
      "name": "Input Files",
      "description": "Specify input files to display or modify",
      "command-line-flag": "-infiles",
      "value-key": "[INFILES]",
      "type": "File",
      "list": true,
      "optional": false
    },
    {
      "id": "ge_all",
      "name": "GE All",
      "description": "Display GE header and extras info",
      "command-line-flag": "-ge_all",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE_ALL]"
    },
    {
      "id": "ge_header",
      "name": "GE Header",
      "description": "Display GE header info",
      "command-line-flag": "-ge_header",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE_HEADER]"
    },
    {
      "id": "ge_extras",
      "name": "GE Extras",
      "description": "Display extra GE image info",
      "command-line-flag": "-ge_extras",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE_EXTRAS]"
    },
    {
      "id": "ge_uv17",
      "name": "GE UV17",
      "description": "Display the value of uv17 (the run #)",
      "command-line-flag": "-ge_uv17",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE_UV17]"
    },
    {
      "id": "ge_run",
      "name": "GE Run",
      "description": "Display the value of uv17 (the run #)",
      "command-line-flag": "-ge_run",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE_RUN]"
    },
    {
      "id": "ge_off",
      "name": "GE Offsets",
      "description": "Display file offsets for various fields",
      "command-line-flag": "-ge_off",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE_OFF]"
    },
    {
      "id": "ge4_all",
      "name": "GEMS 4.x All",
      "description": "Display GEMS 4.x series and image headers",
      "command-line-flag": "-ge4_all",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE4_ALL]"
    },
    {
      "id": "ge4_image",
      "name": "GEMS 4.x Image Header",
      "description": "Display GEMS 4.x image header",
      "command-line-flag": "-ge4_image",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE4_IMAGE]"
    },
    {
      "id": "ge4_series",
      "name": "GEMS 4.x Series Header",
      "description": "Display GEMS 4.x series header",
      "command-line-flag": "-ge4_series",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE4_SERIES]"
    },
    {
      "id": "ge4_study",
      "name": "GEMS 4.x Study Header",
      "description": "Display GEMS 4.x study header",
      "command-line-flag": "-ge4_study",
      "type": "Flag",
      "optional": true,
      "value-key": "[GE4_STUDY]"
    },
    {
      "id": "def_ana_hdr",
      "name": "Define ANALYZE Header",
      "description": "Display the definition of an ANALYZE header",
      "command-line-flag": "-def_ana_hdr",
      "type": "Flag",
      "optional": true,
      "value-key": "[DEF_ANA_HDR]"
    },
    {
      "id": "diff_ana_hdrs",
      "name": "Diff ANALYZE Headers",
      "description": "Display field differences between 2 headers",
      "command-line-flag": "-diff_ana_hdrs",
      "type": "Flag",
      "optional": true,
      "value-key": "[DIFF_ANA_HDRS]"
    },
    {
      "id": "disp_ana_hdr",
      "name": "Display ANALYZE Header",
      "description": "Display ANALYZE headers",
      "command-line-flag": "-disp_ana_hdr",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_ANA_HDR]"
    },
    {
      "id": "hex",
      "name": "Hexadecimal Display",
      "description": "Display field values in hexadecimal",
      "command-line-flag": "-hex",
      "type": "Flag",
      "optional": true,
      "value-key": "[HEX]"
    },
    {
      "id": "mod_ana_hdr",
      "name": "Modify ANALYZE Header",
      "description": "Modify ANALYZE headers",
      "command-line-flag": "-mod_ana_hdr",
      "type": "Flag",
      "optional": true,
      "value-key": "[MOD_ANA_HDR]"
    },
    {
      "id": "mod_field",
      "name": "Modify Field",
      "description": "Specify a field and value(s) to modify",
      "command-line-flag": "-mod_field",
      "value-key": "[MOD_FIELD]",
      "type": "String",
      "optional": true
    },
    {
      "id": "prefix",
      "name": "Prefix",
      "description": "Specify an output filename",
      "command-line-flag": "-prefix",
      "value-key": "[PREFIX]",
      "type": "String",
      "optional": true
    },
    {
      "id": "overwrite",
      "name": "Overwrite",
      "description": "Specify to overwrite the input file(s)",
      "command-line-flag": "-overwrite",
      "type": "Flag",
      "optional": true,
      "value-key": "[OVERWRITE]"
    },
    {
      "id": "show_bad_all",
      "name": "Show Bad All",
      "description": "Show lines with whitespace after '\\'",
      "command-line-flag": "-show_bad_all",
      "type": "Flag",
      "optional": true,
      "value-key": "[SHOW_BAD_ALL]"
    },
    {
      "id": "show_bad_backslash",
      "name": "Show Bad Backslash",
      "description": "Show lines with whitespace after '\\'",
      "command-line-flag": "-show_bad_backslash",
      "type": "Flag",
      "optional": true,
      "value-key": "[SHOW_BAD_BACKSLASH]"
    },
    {
      "id": "show_bad_char",
      "name": "Show Bad Characters",
      "description": "Show any non-printable characters",
      "command-line-flag": "-show_bad_char",
      "type": "Flag",
      "optional": true,
      "value-key": "[SHOW_BAD_CHAR]"
    },
    {
      "id": "show_file_type",
      "name": "Show File Type",
      "description": "Print file type of UNIX, Mac or DOS",
      "command-line-flag": "-show_file_type",
      "type": "Flag",
      "optional": true,
      "value-key": "[SHOW_FILE_TYPE]"
    },
    {
      "id": "fix_rich_quotes",
      "name": "Fix Rich Quotes",
      "description": "Replace rich-text quotes with ASCII",
      "command-line-flag": "-fix_rich_quotes",
      "value-key": "[FIX_RICH_QUOTES]",
      "type": "String",
      "optional": true
    },
    {
      "id": "test",
      "name": "Test",
      "description": "Short for -show_bad_all. Check script files for known issues",
      "command-line-flag": "-test",
      "type": "Flag",
      "optional": true,
      "value-key": "[TEST]"
    },
    {
      "id": "length",
      "name": "Length",
      "description": "Specify the number of bytes to print/modify",
      "command-line-flag": "-length",
      "value-key": "[LENGTH]",
      "type": "Number",
      "optional": true
    },
    {
      "id": "mod_data",
      "name": "Modify Data",
      "description": "Specify a string to modify the data to",
      "command-line-flag": "-mod_data",
      "value-key": "[MOD_DATA]",
      "type": "String",
      "optional": true
    },
    {
      "id": "mod_type",
      "name": "Modify Type",
      "description": "Specify the data type to write to the file",
      "command-line-flag": "-mod_type",
      "value-key": "[MOD_TYPE]",
      "type": "String",
      "optional": true
    },
    {
      "id": "offset",
      "name": "Offset",
      "description": "Specify the offset into each file",
      "command-line-flag": "-offset",
      "value-key": "[OFFSET]",
      "type": "Number",
      "optional": true
    },
    {
      "id": "quiet",
      "name": "Quiet",
      "description": "Do not output header information",
      "command-line-flag": "-quiet",
      "type": "Flag",
      "optional": true,
      "value-key": "[QUIET]"
    },
    {
      "id": "disp_hex",
      "name": "Display Hex",
      "description": "Display bytes in hex",
      "command-line-flag": "-disp_hex",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_HEX]"
    },
    {
      "id": "disp_hex1",
      "name": "Display Hex 1",
      "description": "Display bytes in hex",
      "command-line-flag": "-disp_hex1",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_HEX1]"
    },
    {
      "id": "disp_hex2",
      "name": "Display Hex 2",
      "description": "Display 2-byte integers in hex",
      "command-line-flag": "-disp_hex2",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_HEX2]"
    },
    {
      "id": "disp_hex4",
      "name": "Display Hex 4",
      "description": "Display 4-byte integers in hex",
      "command-line-flag": "-disp_hex4",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_HEX4]"
    },
    {
      "id": "disp_int2",
      "name": "Display Int 2",
      "description": "Display 2-byte integers",
      "command-line-flag": "-disp_int2",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_INT2]"
    },
    {
      "id": "disp_int4",
      "name": "Display Int 4",
      "description": "Display 4-byte integers",
      "command-line-flag": "-disp_int4",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_INT4]"
    },
    {
      "id": "disp_real4",
      "name": "Display Real 4",
      "description": "Display 4-byte real numbers",
      "command-line-flag": "-disp_real4",
      "type": "Flag",
      "optional": true,
      "value-key": "[DISP_REAL4]"
    },
    {
      "id": "swap_bytes",
      "name": "Swap Bytes",
      "description": "Use byte-swapping on numbers",
      "command-line-flag": "-swap_bytes",
      "type": "Flag",
      "optional": true,
      "value-key": "[SWAP_BYTES]"
    }
  ],
  "output-files": [
    {
      "id": "modified_file",
      "name": "Modified File",
      "path-template": "[PREFIX]",
      "description": "The modified output file",
      "optional": true
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  }
}