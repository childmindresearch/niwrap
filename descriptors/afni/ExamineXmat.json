{
  "tool-version": "24.2.06",
  "name": "ExamineXmat",
  "author": "AFNI Developers",
  "description": "A program for examining the design matrix generated by 3dDeconvolve.",
  "command-line": "ExamineXmat [INPUT_FILE] [INTERACTIVE] [PREFIX] [CPREFIX] [PPREFIX] [SELECT] [MSG_TRACE] [VERBOSITY]",
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  },
  "inputs": [
    {
      "description": "xmat file to plot",
      "value-key": "[INPUT_FILE]",
      "type": "File",
      "optional": true,
      "id": "input_file",
      "name": "Input file",
      "command-line-flag": "-input"
    },
    {
      "command-line-flag": "-interactive",
      "description": "Run ExamineXmat in interactive mode. This is the default if -prefix is not given. If -interactive is used with -prefix, the last plot you see is the plot saved to file.",
      "value-key": "[INTERACTIVE]",
      "type": "Flag",
      "optional": true,
      "id": "interactive",
      "name": "Interactive mode"
    },
    {
      "description": "Prefix of plot image and cor image",
      "value-key": "[PREFIX]",
      "type": "String",
      "optional": true,
      "id": "prefix",
      "name": "Prefix",
      "command-line-flag": "-prefix"
    },
    {
      "description": "Prefix of cor image only",
      "value-key": "[CPREFIX]",
      "type": "String",
      "optional": true,
      "id": "cprefix",
      "name": "Cor image prefix",
      "command-line-flag": "-cprefix"
    },
    {
      "description": "Prefix of plot image only",
      "value-key": "[PPREFIX]",
      "type": "String",
      "optional": true,
      "id": "pprefix",
      "name": "Plot image prefix",
      "command-line-flag": "-pprefix"
    },
    {
      "description": "What to plot. Selection strings to specify regressors.",
      "value-key": "[SELECT]",
      "type": "String",
      "optional": true,
      "id": "select",
      "name": "Select",
      "command-line-flag": "-select"
    },
    {
      "command-line-flag": "-msg.trace",
      "description": "Output trace information along with errors and notices.",
      "value-key": "[MSG_TRACE]",
      "type": "Flag",
      "optional": true,
      "id": "msg_trace",
      "name": "Message trace flag"
    },
    {
      "description": "Verbosity level. 0 for quiet, 1 or more for talkative.",
      "value-key": "[VERBOSITY]",
      "type": "Number",
      "optional": true,
      "id": "verbosity",
      "name": "Verbosity level",
      "command-line-flag": "-verb",
      "minimum": 0
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "[PREFIX].jpg",
      "description": "Output plot image",
      "optional": true,
      "id": "plot_image",
      "name": "Plot image output"
    },
    {
      "path-template": "[PREFIX].png",
      "description": "Output plot image",
      "optional": true,
      "id": "plot_image_png",
      "name": "Plot image output (PNG)"
    },
    {
      "path-template": "[PREFIX].pdf",
      "description": "Output plot image",
      "optional": true,
      "id": "plot_image_pdf",
      "name": "Plot image output (PDF)"
    },
    {
      "path-template": "[CPREFIX].jpg",
      "description": "Output cor image",
      "optional": true,
      "id": "cor_image",
      "name": "Cor image output"
    },
    {
      "path-template": "[PPREFIX].jpg",
      "description": "Output plot image",
      "optional": true,
      "id": "plot_image_prefix",
      "name": "Plot image output (plot image only)"
    }
  ],
  "url": "https://afni.nimh.nih.gov/"
}
