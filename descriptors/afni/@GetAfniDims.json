{
  "tool-version": "24.2.06",
  "name": "@GetAfniDims",
  "author": "AFNI Developers",
  "description": "A utility tool to return dimensions of AFNI dataset",
  "command-line": "@GetAfniDims [INPUT_DSET]",
  "inputs": [
    {
      "description": "Input AFNI dataset",
      "value-key": "[INPUT_DSET]",
      "type": "File",
      "optional": false,
      "id": "input_dset",
      "name": "Input dataset"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "dims_output.txt",
      "description": "Text file containing the dimensions of the input dataset",
      "optional": false,
      "id": "dims_output",
      "name": "Dimensions output file"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  },
  "url": "https://afni.nimh.nih.gov/"
}
