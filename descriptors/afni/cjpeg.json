{
  "tool-version": "24.2.06",
  "name": "cjpeg",
  "author": "AFNI Developers",
  "description": "Compresses an image file to a JPEG file",
  "command-line": "cjpeg [QUALITY] [GRAYSCALE] [OPTIMIZE] [BASELINE] [PROGRESSIVE] [OUTFILE] [INFILE]",
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  },
  "inputs": [
    {
      "description": "Input image file",
      "value-key": "[INFILE]",
      "type": "File",
      "optional": false,
      "id": "infile",
      "name": "Input file"
    },
    {
      "description": "Output JPEG file",
      "value-key": "[OUTFILE]",
      "type": "String",
      "optional": false,
      "id": "outfile",
      "name": "Output file"
    },
    {
      "command-line-flag": "-quality",
      "description": "Quality of JPEG image (0-100)",
      "value-key": "[QUALITY]",
      "type": "Number",
      "minimum": 0,
      "maximum": 100,
      "optional": true,
      "id": "quality",
      "name": "Quality"
    },
    {
      "command-line-flag": "-grayscale",
      "description": "Create a grayscale JPEG file",
      "value-key": "[GRAYSCALE]",
      "type": "Flag",
      "optional": true,
      "id": "grayscale",
      "name": "Grayscale"
    },
    {
      "command-line-flag": "-optimize",
      "description": "Optimize Huffman table",
      "value-key": "[OPTIMIZE]",
      "type": "Flag",
      "optional": true,
      "id": "optimize",
      "name": "Optimize"
    },
    {
      "command-line-flag": "-baseline",
      "description": "Create a baseline JPEG file",
      "value-key": "[BASELINE]",
      "type": "Flag",
      "optional": true,
      "id": "baseline",
      "name": "Baseline"
    },
    {
      "command-line-flag": "-progressive",
      "description": "Create a progressive JPEG file",
      "value-key": "[PROGRESSIVE]",
      "type": "Flag",
      "optional": true,
      "id": "progressive",
      "name": "Progressive"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "The output JPEG file",
      "optional": false,
      "id": "outfile",
      "name": "Output file"
    }
  ],
  "url": "https://afni.nimh.nih.gov/"
}
