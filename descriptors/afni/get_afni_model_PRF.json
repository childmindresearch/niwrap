{
  "tool-version": "24.2.06",
  "name": "get_afni_model_PRF",
  "author": "AFNI Developers",
  "description": "A tool to get AFNI model parameters assuming a PRF framework",
  "command-line": "get_afni_model_PRF [AMPLITUDE] [X_COORD] [Y_COORD] [SIGMA]",
  "inputs": [
    {
      "description": "Amplitude for the AFNI model",
      "value-key": "[AMPLITUDE]",
      "type": "Number",
      "optional": false,
      "id": "amplitude",
      "name": "Amplitude"
    },
    {
      "description": "X-coordinate for the AFNI model",
      "value-key": "[X_COORD]",
      "type": "Number",
      "optional": false,
      "id": "x_coord",
      "name": "X-coordinate"
    },
    {
      "description": "Y-coordinate for the AFNI model",
      "value-key": "[Y_COORD]",
      "type": "Number",
      "optional": false,
      "id": "y_coord",
      "name": "Y-coordinate"
    },
    {
      "description": "Sigma value for the AFNI model",
      "value-key": "[SIGMA]",
      "type": "Number",
      "optional": false,
      "id": "sigma",
      "name": "Sigma"
    }
  ],
  "schema-version": "0.5",
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  },
  "url": "https://afni.nimh.nih.gov/"
}
