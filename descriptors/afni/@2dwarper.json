{
  "tool-version": "24.2.06",
  "name": "@2dwarper",
  "author": "AFNI Developers",
  "description": "2D image warping tool",
  "command-line": "@2dwarper [INPUT_DATASET]",
  "inputs": [
    {
      "description": "Input dataset (e.g., image to be warped)",
      "value-key": "[INPUT_DATASET]",
      "type": "File",
      "optional": false,
      "id": "input_dataset",
      "name": "Input dataset"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "warped_output",
      "description": "Warped output image from the dataset",
      "optional": true,
      "id": "output_file",
      "name": "Warped output file"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "afni/afni_make_build:AFNI_24.2.06"
  },
  "url": "https://afni.nimh.nih.gov/"
}
