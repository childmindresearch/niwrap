{
  "name": "predict_v1.sh",
  "description": "A script for predicting brain images using registration",
  "command-line": "predict_v1.sh [TEMPLATE] [INFLATED_SURFACE_FLAG] [HEMISPHERE] [PRINT_MODE_FLAG] [SUBJECTS]...",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-t",
      "description": "Target image for registration (exvivo or invivo [default])",
      "value-key": "[TEMPLATE]",
      "type": "String",
      "optional": true,
      "id": "template",
      "name": "Registration Template"
    },
    {
      "command-line-flag": "-i",
      "description": "Don't use inflated surface as initial registration (backward compatibility)",
      "value-key": "[INFLATED_SURFACE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "inflated_surface_flag",
      "name": "Inflated Surface Flag"
    },
    {
      "command-line-flag": "-h",
      "description": "Hemisphere (rh or lh) default is both hemispheres",
      "value-key": "[HEMISPHERE]",
      "type": "String",
      "optional": true,
      "id": "hemisphere",
      "name": "Hemisphere"
    },
    {
      "command-line-flag": "-p",
      "description": "Print mode (do not run commands, just print them)",
      "value-key": "[PRINT_MODE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "print_mode_flag",
      "name": "Print Mode Flag"
    },
    {
      "description": "Subjects for prediction",
      "value-key": "[SUBJECTS]",
      "type": "String",
      "list": true,
      "optional": false,
      "id": "subjects",
      "name": "Subjects"
    },
    {
      "command-line-flag": "-u",
      "description": "Print usage",
      "value-key": "[USAGE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "usage_flag",
      "name": "Usage Flag"
    }
  ],
  "author": "FreeSurfer Developers",
  "tool-version": "7.4.1",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
