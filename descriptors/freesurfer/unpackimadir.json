{
  "tool-version": "7.4.1",
  "name": "unpackimadir",
  "author": "FreeSurfer Developers",
  "description": "Unpack image directories.",
  "command-line": "unpackimadir [SOURCE_DIRECTORY] [TARGET_DIRECTORY]",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-src",
      "description": "Source directory containing the images to be unpacked",
      "value-key": "[SOURCE_DIRECTORY]",
      "type": "String",
      "optional": false,
      "id": "source_directory",
      "name": "Source Directory"
    },
    {
      "command-line-flag": "-targ",
      "description": "Target directory where the unpacked images will be stored",
      "value-key": "[TARGET_DIRECTORY]",
      "type": "String",
      "optional": false,
      "id": "target_directory",
      "name": "Target Directory"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
