{
  "tool-version": "7.4.1",
  "name": "sphere_subject-lh",
  "author": "FreeSurfer Developers",
  "descriptor-url": "https://github.com/freesurfer/freesurfer",
  "description": "Tool for processing spherical representations in FreeSurfer",
  "command-line": "sphere_subject[LICENSE_FILE]",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "Path to the FreeSurfer license file",
      "value-key": "[LICENSE_FILE]",
      "type": "File",
      "optional": false,
      "id": "license_file",
      "name": "License file",
      "default-value": "/usr/local/freesurfer/.license",
      "command-line-flag": "-lh"
    }
  ],
  "url": "https://github.com/freesurfer/freesurfer"
}
