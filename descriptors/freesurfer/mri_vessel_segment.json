{
  "tool-version": "7.4.1",
  "name": "mri_vessel_segment",
  "author": "FreeSurfer Developers",
  "descriptor-url": "https://github.com/freesurfer/freesurfer",
  "description": "MRI vessel segmentation tool",
  "command-line": "mri_vessel_segment [T1_IMAGE] [T2_IMAGE] [ASEG_FILE] [OUTPUT_FILE] [SHAPE_FLAG]",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-t1",
      "description": "T1-weighted input image",
      "value-key": "[T1_IMAGE]",
      "type": "File",
      "optional": false,
      "id": "t1_image",
      "name": "T1 Image"
    },
    {
      "command-line-flag": "-t2",
      "description": "T2-weighted input image",
      "value-key": "[T2_IMAGE]",
      "type": "File",
      "optional": false,
      "id": "t2_image",
      "name": "T2 Image"
    },
    {
      "command-line-flag": "-aseg",
      "description": "Anatomical segmentation file",
      "value-key": "[ASEG_FILE]",
      "type": "File",
      "optional": false,
      "id": "aseg_file",
      "name": "ASEG File"
    },
    {
      "command-line-flag": "-o",
      "description": "Output file",
      "value-key": "[OUTPUT_FILE]",
      "type": "String",
      "optional": false,
      "id": "output_file",
      "name": "Output File"
    },
    {
      "command-line-flag": "--shape",
      "description": "Use shape constraints during segmentation",
      "value-key": "[SHAPE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "shape_flag",
      "name": "Shape Flag"
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "Segmented vessel output file",
      "optional": false,
      "id": "segmented_output",
      "name": "Segmented Output File"
    }
  ],
  "url": "https://github.com/freesurfer/freesurfer"
}
