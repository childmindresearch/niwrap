{
  "tool-version": "7.4.1",
  "name": "flirt.newdefault.20080811.sch",
  "author": "FreeSurfer Developers",
  "description": "FLIRT (FMRIB's Linear Image Registration Tool) new default configuration script.",
  "command-line": "flirt.newdefault.20080811.sch [TERM_OPTION] [CURSES_FLAG] [SCROLLBACK_FLAG]",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-T",
      "description": "Use this terminal type instead of $TERM",
      "value-key": "[TERM_OPTION]",
      "type": "String",
      "optional": true,
      "id": "term_option",
      "name": "Terminal Option"
    },
    {
      "command-line-flag": "-V",
      "description": "Print curses-version",
      "value-key": "[CURSES_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "curses_flag",
      "name": "Curses Version Flag"
    },
    {
      "command-line-flag": "-x",
      "description": "Do not try to clear scrollback",
      "value-key": "[SCROLLBACK_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "scrollback_flag",
      "name": "Clear Scrollback Flag"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
