{
  "tool-version": "7.4.1",
  "name": "mri_wbc",
  "description": "A tool for working with functional brain imaging data on surfaces and volumes.",
  "command-line": "mri_wbc [FUNCTIONAL_VOLUME] [VOLUME_MASK] [LH_FUNCTIONAL_SURFACE] [LH_SURFACE] [LH_INFLATED] [LH_MASK] [LH_LABEL] [RH_FUNCTIONAL_SURFACE] [RH_SURFACE] [RH_INFLATED] [RH_MASK] [RH_LABEL] [RHO_THRESHOLD] [DIST_THRESHOLD] [THREADS] [DEBUG] [CHECKOPTS]",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "--fvol",
      "description": "Functional volume file",
      "value-key": "[FUNCTIONAL_VOLUME]",
      "type": "File",
      "optional": false,
      "id": "functional_volume",
      "name": "Functional Volume"
    },
    {
      "command-line-flag": "--volmask",
      "description": "Mask for functional volume",
      "value-key": "[VOLUME_MASK]",
      "type": "File",
      "optional": true,
      "id": "volume_mask",
      "name": "Volume Mask"
    },
    {
      "command-line-flag": "--lh",
      "description": "Left hemisphere functional surface file",
      "value-key": "[LH_FUNCTIONAL_SURFACE]",
      "type": "File",
      "optional": false,
      "id": "lh_functional_surface",
      "name": "Left Hemisphere Functional Surface"
    },
    {
      "command-line-flag": "--lh",
      "description": "Left hemisphere surface file",
      "value-key": "[LH_SURFACE]",
      "type": "File",
      "optional": false,
      "id": "lh_surface",
      "name": "Left Hemisphere Surface"
    },
    {
      "command-line-flag": "--lh",
      "description": "Optional left hemisphere inflated surface file",
      "value-key": "[LH_INFLATED]",
      "type": "File",
      "optional": true,
      "id": "lh_inflated",
      "name": "Left Hemisphere Inflated Surface"
    },
    {
      "command-line-flag": "--lhmask",
      "description": "Mask for left hemisphere functional surface",
      "value-key": "[LH_MASK]",
      "type": "File",
      "optional": true,
      "id": "lh_mask",
      "name": "Left Hemisphere Mask"
    },
    {
      "command-line-flag": "--lhlabel",
      "description": "Label mask for left hemisphere functional surface",
      "value-key": "[LH_LABEL]",
      "type": "File",
      "optional": true,
      "id": "lh_label",
      "name": "Left Hemisphere Label Mask"
    },
    {
      "command-line-flag": "--rh",
      "description": "Right hemisphere functional surface file",
      "value-key": "[RH_FUNCTIONAL_SURFACE]",
      "type": "File",
      "optional": false,
      "id": "rh_functional_surface",
      "name": "Right Hemisphere Functional Surface"
    },
    {
      "command-line-flag": "--rh",
      "description": "Right hemisphere surface file",
      "value-key": "[RH_SURFACE]",
      "type": "File",
      "optional": false,
      "id": "rh_surface",
      "name": "Right Hemisphere Surface"
    },
    {
      "command-line-flag": "--rh",
      "description": "Optional right hemisphere inflated surface file",
      "value-key": "[RH_INFLATED]",
      "type": "File",
      "optional": true,
      "id": "rh_inflated",
      "name": "Right Hemisphere Inflated Surface"
    },
    {
      "command-line-flag": "--rhmask",
      "description": "Mask for right hemisphere functional surface",
      "value-key": "[RH_MASK]",
      "type": "File",
      "optional": true,
      "id": "rh_mask",
      "name": "Right Hemisphere Mask"
    },
    {
      "command-line-flag": "--rhlabel",
      "description": "Label mask for right hemisphere functional surface",
      "value-key": "[RH_LABEL]",
      "type": "File",
      "optional": true,
      "id": "rh_label",
      "name": "Right Hemisphere Label Mask"
    },
    {
      "command-line-flag": "--rho",
      "description": "Rho threshold value",
      "value-key": "[RHO_THRESHOLD]",
      "type": "Number",
      "optional": true,
      "id": "rho_threshold",
      "name": "Rho Threshold"
    },
    {
      "command-line-flag": "--dist",
      "description": "Distance threshold value",
      "value-key": "[DIST_THRESHOLD]",
      "type": "Number",
      "optional": true,
      "id": "dist_threshold",
      "name": "Distance Threshold"
    },
    {
      "command-line-flag": "--threads",
      "description": "Number of threads to use",
      "value-key": "[THREADS]",
      "type": "Number",
      "optional": true,
      "id": "threads",
      "name": "Threads"
    },
    {
      "command-line-flag": "--debug",
      "description": "Turn on debugging",
      "value-key": "[DEBUG]",
      "type": "Flag",
      "optional": true,
      "id": "debug",
      "name": "Debug Flag"
    },
    {
      "command-line-flag": "--checkopts",
      "description": "Check options and exit without running",
      "value-key": "[CHECKOPTS]",
      "type": "Flag",
      "optional": true,
      "id": "checkopts",
      "name": "Check Options Flag"
    }
  ],
  "author": "FreeSurfer Developers",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
