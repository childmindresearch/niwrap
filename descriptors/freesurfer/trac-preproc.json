{
  "tool-version": "7.4.1",
  "name": "trac-preproc",
  "author": "FreeSurfer Developers",
  "description": "Tractography pre-processing for a single subject.",
  "command-line": "trac-preproc [DMRIRC_FILE] [LOG_FILE] [NOLOG] [CMD_FILE] [NOCMD] [NO_ISRUNNING] [UMASK] [GROUP_ID] [ALLOW_CORE_DUMP] [DEBUG] [DONTRUN] [VERSION]",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-c",
      "description": "dmrirc file (see dmrirc.example)",
      "value-key": "[DMRIRC_FILE]",
      "type": "File",
      "optional": false,
      "id": "dmrirc_file",
      "name": "dmrirc file"
    },
    {
      "command-line-flag": "-log",
      "description": "Log file; default is trac-all.log in the same dir as dmrirc",
      "value-key": "[LOG_FILE]",
      "type": "String",
      "optional": true,
      "id": "log_file",
      "name": "Log file"
    },
    {
      "command-line-flag": "-nolog",
      "description": "Do not save a log file",
      "value-key": "[NOLOG]",
      "type": "Flag",
      "optional": true,
      "id": "nolog",
      "name": "No log flag"
    },
    {
      "command-line-flag": "-cmd",
      "description": "Cmd file; default is trac-all.cmd in the same dir as dmrirc",
      "value-key": "[CMD_FILE]",
      "type": "String",
      "optional": true,
      "id": "cmd_file",
      "name": "Cmd file"
    },
    {
      "command-line-flag": "-nocmd",
      "description": "Do not save a cmd file",
      "value-key": "[NOCMD]",
      "type": "Flag",
      "optional": true,
      "id": "nocmd",
      "name": "No cmd flag"
    },
    {
      "command-line-flag": "-no-isrunning",
      "description": "Do not check whether this subject is currently being processed",
      "value-key": "[NO_ISRUNNING]",
      "type": "Flag",
      "optional": true,
      "id": "no_isrunning",
      "name": "No isrunning flag"
    },
    {
      "command-line-flag": "-umask",
      "description": "Set unix file permission mask (default 002)",
      "value-key": "[UMASK]",
      "type": "String",
      "optional": true,
      "id": "umask",
      "name": "Umask"
    },
    {
      "command-line-flag": "-grp",
      "description": "Check that current group is alpha groupid",
      "value-key": "[GROUP_ID]",
      "type": "String",
      "optional": true,
      "id": "group_id",
      "name": "Group ID"
    },
    {
      "command-line-flag": "-allowcoredump",
      "description": "Set coredump limit to unlimited",
      "value-key": "[ALLOW_CORE_DUMP]",
      "type": "Flag",
      "optional": true,
      "id": "allow_core_dump",
      "name": "Allow coredump flag"
    },
    {
      "command-line-flag": "-debug",
      "description": "Generate much more output",
      "value-key": "[DEBUG]",
      "type": "Flag",
      "optional": true,
      "id": "debug",
      "name": "Debug flag"
    },
    {
      "command-line-flag": "-dontrun",
      "description": "Do everything but execute each command",
      "value-key": "[DONTRUN]",
      "type": "Flag",
      "optional": true,
      "id": "dontrun",
      "name": "Dontrun flag"
    },
    {
      "command-line-flag": "-version",
      "description": "Print version of this script and exit",
      "value-key": "[VERSION]",
      "type": "Flag",
      "optional": true,
      "id": "version",
      "name": "Version flag"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
