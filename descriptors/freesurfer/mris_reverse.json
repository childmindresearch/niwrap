{
  "name": "mris_reverse",
  "description": "This tool reverses a cortical surface.",
  "command-line": "/usr/local/freesurfer/bin/mris_reverse [INPUT_SURFACE] [OUTPUT_SURFACE]",
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "Input cortical surface file.",
      "value-key": "[INPUT_SURFACE]",
      "type": "File",
      "optional": false,
      "id": "input_surface",
      "name": "Input Surface"
    },
    {
      "description": "Output cortical surface file.",
      "value-key": "[OUTPUT_SURFACE]",
      "type": "String",
      "optional": false,
      "id": "output_surface",
      "name": "Output Surface"
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_SURFACE].surf",
      "description": "Reversed cortical surface file.",
      "optional": false,
      "id": "reversed_surface",
      "name": "Reversed Surface"
    }
  ],
  "author": "FreeSurfer Developers",
  "tool-version": "7.4.1",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}