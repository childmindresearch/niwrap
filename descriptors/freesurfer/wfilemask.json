{
  "tool-version": "7.4.1",
  "name": "wfilemask",
  "author": "FreeSurfer Developers",
  "description": "Zero-out regions of a surface value file (.w file) using a label.",
  "command-line": "wfilemask [W_FILE] [LABEL_FILE] [OUTPUT_FILE] [PERMISSION_MASK] [HELP_FLAG] [VERSION_FLAG]",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-w",
      "description": "Input w file",
      "value-key": "[W_FILE]",
      "type": "File",
      "optional": false,
      "id": "w_file",
      "name": "Input W file"
    },
    {
      "command-line-flag": "-l",
      "description": "Label file to use as a mask",
      "value-key": "[LABEL_FILE]",
      "type": "File",
      "optional": false,
      "id": "label_file",
      "name": "Label File"
    },
    {
      "command-line-flag": "-o",
      "description": "Output w file",
      "value-key": "[OUTPUT_FILE]",
      "type": "String",
      "optional": false,
      "id": "output_file",
      "name": "Output W file"
    },
    {
      "command-line-flag": "-umask",
      "description": "Set Unix file permission mask",
      "value-key": "[PERMISSION_MASK]",
      "type": "String",
      "optional": true,
      "id": "permission_mask",
      "name": "Permission Mask"
    },
    {
      "command-line-flag": "-help",
      "description": "Display help message and exit",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "help_flag",
      "name": "Help Flag"
    },
    {
      "command-line-flag": "-version",
      "description": "Print version and exit",
      "value-key": "[VERSION_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "version_flag",
      "name": "Version Flag"
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "Output file with specified label regions zeroed out",
      "optional": false,
      "id": "output_w_file",
      "name": "Output W File"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}