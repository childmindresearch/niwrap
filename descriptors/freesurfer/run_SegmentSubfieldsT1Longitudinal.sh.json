  "command-line": "run_SegmentSubfieldsT1Longitudinal.sh [DEPLOYED_MCR_ROOT] [ADDITIONAL_ARGS]",
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "Root directory for the deployed MATLAB Runtime environment.",
      "value-key": "[DEPLOYED_MCR_ROOT]",
      "type": "String",
      "optional": false,
      "id": "deployed_mcr_root",
      "name": "Deployed MCR Root"
    },
    {
      "description": "Additional arguments for the script.",
      "value-key": "[ADDITIONAL_ARGS]",
      "type": "String",
      "optional": true,
      "id": "additional_args",
      "name": "Additional Arguments"
    }
  ],
  "tool-version": "7.4.1",
  "author": "FreeSurfer Developers",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
