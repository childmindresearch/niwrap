{
  "name": "listsubj",
  "description": "List subject IDs in SUBJECT_DIR",
  "command-line": "listsubj [OPTIONS] [SUBJECT_DIR]",
  "schema-version": "0.5",
  "inputs": [
    {
      "id": "subject_dir",
      "name": "Subject Directory",
      "type": "String",
      "optional": false,
      "description": "Directory where subjects are stored",
      "value-key": "[SUBJECT_DIR]"
    },
    {
      "command-line-flag": "-c",
      "id": "cross",
      "name": "Cross-sectional stream",
      "type": "Flag",
      "optional": true,
      "description": "Only cross-sectional stream",
      "value-key": "[CROSS]"
    },
    {
      "command-line-flag": "-b",
      "id": "base",
      "name": "Base stream",
      "type": "Flag",
      "optional": true,
      "description": "Only base stream",
      "value-key": "[BASE]"
    },
    {
      "command-line-flag": "-l",
      "id": "long",
      "name": "Longitudinal stream",
      "type": "Flag",
      "optional": true,
      "description": "Only longitudinal stream",
      "value-key": "[LONG]"
    },
    {
      "command-line-flag": "-d",
      "id": "done",
      "name": "Completed scripts",
      "type": "Flag",
      "optional": true,
      "description": "Only subject IDs with scripts/recon-all.done",
      "value-key": "[DONE]"
    },
    {
      "command-line-flag": "-e",
      "id": "error",
      "name": "Error in scripts",
      "type": "Flag",
      "optional": true,
      "description": "Only subject IDs with scripts/recon-all.error",
      "value-key": "[ERROR]"
    },
    {
      "command-line-flag": "-r",
      "id": "running",
      "name": "Running scripts",
      "type": "Flag",
      "optional": true,
      "description": "Only subject IDs with scripts/IsRunning.?h",
      "value-key": "[RUNNING]"
    },
    {
      "command-line-flag": "-f",
      "id": "full_path",
      "name": "Full Path",
      "type": "Flag",
      "optional": true,
      "description": "Prepend full absolute path",
      "value-key": "[FULL_PATH]"
    },
    {
      "command-line-flag": "-n",
      "id": "count",
      "name": "Count Subjects",
      "type": "Flag",
      "optional": true,
      "description": "Print number of subjects found",
      "value-key": "[COUNT]"
    },
    {
      "command-line-flag": "-h",
      "id": "help",
      "name": "Help",
      "type": "Flag",
      "optional": true,
      "description": "Show this help text",
      "value-key": "[HELP]"
    }
  ],
  "author": "FreeSurfer Developers",
  "tool-version": "7.4.1",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
