{
  "tool-version": "1.0.0",
  "name": "mccutup",
  "author": "FSL",
  "description": "FSL mccutup tool",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki/",
  "command-line": "/usr/local/fsl/bin/mccutup [OPTIONS] <input>",
  "container-image": {
    "image": "fsl_image",
    "index": "index.docker.io",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Input file",
      "value-key": "<input>",
      "type": "File",
      "optional": false,
      "id": "input",
      "name": "Input file"
    },
    {
      "command-line-flag": "--output",
      "description": "Specify output file name",
      "value-key": "[OUTPUT_FILE]",
      "type": "String",
      "optional": true,
      "id": "output_file",
      "name": "Output file"
    },
    {
      "command-line-flag": "--param1",
      "description": "Parameter 1 description",
      "value-key": "[PARAM1]",
      "type": "String",
      "optional": true,
      "id": "param1",
      "name": "Parameter 1"
    },
    {
      "command-line-flag": "--param2",
      "description": "Parameter 2 description",
      "value-key": "[PARAM2]",
      "type": "String",
      "optional": true,
      "id": "param2",
      "name": "Parameter 2"
    }
  ],
  "schema-version": "0.5",
  "groups": [
    {
      "description": "Main options for mccutup",
      "id": "main_options",
      "members": [
        "output_file",
        "param1",
        "param2"
      ],
      "name": "Main Options"
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "Output file",
      "optional": true,
      "id": "output",
      "name": "Output File"
    }
  ]
}