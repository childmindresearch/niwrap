{
  "tool-version": "1.0.0",
  "name": "slicesmask",
  "author": "Unknown",
  "description": "Tool for masking slices from an image using a mask",
  "command-line": "slicesmask [IMAGE] [MASK] [OUTPUT]",
  "container-image": {
    "image": "unknown",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Input image file",
      "value-key": "[IMAGE]",
      "type": "File",
      "optional": false,
      "id": "image",
      "name": "Input Image"
    },
    {
      "description": "Mask file",
      "value-key": "[MASK]",
      "type": "File",
      "optional": false,
      "id": "mask",
      "name": "Mask File"
    },
    {
      "description": "Output file",
      "value-key": "[OUTPUT]",
      "type": "String",
      "optional": false,
      "id": "output",
      "name": "Output File"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "[OUTPUT]",
      "description": "The output file resulting from applying the mask",
      "optional": false,
      "id": "masked_output",
      "name": "Masked Output File"
    }
  ]
}
