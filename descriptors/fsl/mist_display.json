{
  "tool-version": "6.0.4",
  "name": "mist_display",
  "author": "FMRIB Analysis Group, University of Oxford",
  "descriptor-url": "http://example.com",
  "description": "Tool to show scalars on mesh",
  "command-line": "mist_display [MODE] [--help]",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  },
  "inputs": [
    {
      "description": "Mode for displaying scalars on mesh",
      "value-choices": ["simple", "pvals", "noscalars"],
      "value-key": "[MODE]",
      "type": "String",
      "optional": false,
      "id": "mode",
      "name": "Mode"
    },
    {
      "command-line-flag": "--help",
      "description": "Show help message and exit",
      "value-key": "[--help]",
      "type": "Flag",
      "optional": true,
      "id": "help",
      "name": "Help"
    }
  ],
  "schema-version": "0.5",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki"
}
