{
  "tool-version": "1.0.0",
  "name": "imrm",
  "author": "Oxford Centre for Functional MRI of the Brain (FMRIB)",
  "description": "Remove specified image files.",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki/",
  "command-line": "/usr/local/fsl/bin/imrm [IMAGES_TO_REMOVE]",
  "container-image": {
    "image": "mcin/docker-fsl:latest",
    "index": "index.docker.io",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "List of image names to remove. Filenames can be basenames or full names.",
      "value-key": "[IMAGES_TO_REMOVE]",
      "type": "String",
      "list": true,
      "optional": false,
      "id": "images_to_remove",
      "name": "Images to remove"
    }
  ],
  "schema-version": "0.5"
}