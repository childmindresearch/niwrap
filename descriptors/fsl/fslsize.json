{
  "tool-version": "6.0.4",
  "name": "fslsize",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "Tool to output the size of an image file in FSL",
  "command-line": "fslsize [INPUT_FILE] [SHORT_FORMAT_FLAG]",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  },
  "inputs": [
    {
      "description": "Input image file",
      "value-key": "[INPUT_FILE]",
      "type": "File",
      "optional": false,
      "id": "input_file",
      "name": "Input File"
    },
    {
      "command-line-flag": "-s",
      "description": "Output using short format (one line)",
      "value-key": "[SHORT_FORMAT_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "short_format_flag",
      "name": "Short Format Flag"
    }
  ],
  "schema-version": "0.5",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki"
}
