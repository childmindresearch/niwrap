{
  "tool-version": "6.0.5",
  "name": "fsl_sub_config",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "FSL cluster submission configuration examples",
  "command-line": "fsl_sub_config [CLUSTER_SYSTEM] [HELP_FLAG]",
  "container-image": {
    "image": "mcin/fsl:6.0.5",
    "type": "docker"
  },
  "inputs": [
    {
      "description": "Choice of cluster system configuration example to output. Options are 'shell', 'sge', or 'slurm'.",
      "value-choices": [
        "shell",
        "sge",
        "slurm"
      ],
      "value-key": "[CLUSTER_SYSTEM]",
      "type": "String",
      "optional": false,
      "id": "cluster_system",
      "name": "Cluster system"
    },
    {
      "command-line-flag": "-h",
      "description": "Show help message and exit",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "help_flag",
      "name": "Help flag"
    }
  ],
  "schema-version": "0.5",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki"
}