{
  "name": "overlay",
  "command-line": "overlay [AUTO_THRESH_BG] [BACKGROUND_IMAGE] [BG_THRESH] [FULL_BG_RANGE] [OUT_FILE] [OUT_TYPE] [OUTPUT_TYPE] [STAT_IMAGE] [STAT_IMAGE2] [STAT_THRESH] [STAT_THRESH2] [USE_CHECKERBOARD]",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "Use FSL's overlay command to combine background and statistical images into one volume",
  "inputs": [
    {
      "id": "auto_thresh_bg",
      "name": "Auto thresh bg",
      "type": "Flag",
      "value-key": "[AUTO_THRESH_BG]",
      "command-line-flag": "-a",
      "description": "Automatically threshold the background image.",
      "optional": false
    },
    {
      "id": "background_image",
      "name": "Background image",
      "type": "File",
      "value-key": "[BACKGROUND_IMAGE]",
      "description": "Image to use as background.",
      "optional": false
    },
    {
      "id": "bg_thresh",
      "name": "Bg thresh",
      "type": "Number",
      "list": true,
      "min-list-entries": 2,
      "max-list-entries": 2,
      "value-key": "[BG_THRESH]",
      "description": "(a float, a float). Min and max values for background intensity.",
      "optional": false
    },
    {
      "id": "full_bg_range",
      "name": "Full bg range",
      "type": "Flag",
      "value-key": "[FULL_BG_RANGE]",
      "command-line-flag": "-A",
      "description": "Use full range of background image.",
      "optional": false
    },
    {
      "id": "out_file",
      "name": "Out file",
      "type": "String",
      "value-key": "[OUT_FILE]",
      "description": "Combined image volume.",
      "optional": true
    },
    {
      "id": "out_type",
      "name": "Out type",
      "type": "String",
      "value-key": "[OUT_TYPE]",
      "description": "'float' or 'int'. Write output with float or int.",
      "optional": true,
      "default-value": "float",
      "value-choices": [
        "float",
        "int"
      ]
    },
    {
      "id": "output_type",
      "name": "Output type",
      "type": "String",
      "value-key": "[OUTPUT_TYPE]",
      "description": "'nifti' or 'nifti_pair' or 'nifti_gz' or 'nifti_pair_gz'. Fsl output type.",
      "optional": true,
      "value-choices": [
        "NIFTI",
        "NIFTI_PAIR",
        "NIFTI_GZ",
        "NIFTI_PAIR_GZ"
      ]
    },
    {
      "id": "stat_image",
      "name": "Stat image",
      "type": "File",
      "value-key": "[STAT_IMAGE]",
      "description": "Statistical image to overlay in color.",
      "optional": false
    },
    {
      "id": "stat_image2",
      "name": "Stat image2",
      "type": "File",
      "value-key": "[STAT_IMAGE2]",
      "description": "Second statistical image to overlay in color.",
      "optional": true
    },
    {
      "id": "stat_thresh",
      "name": "Stat thresh",
      "type": "Number",
      "list": true,
      "min-list-entries": 2,
      "max-list-entries": 2,
      "value-key": "[STAT_THRESH]",
      "description": "(a float, a float). Min and max values for the statistical overlay.",
      "optional": false
    },
    {
      "id": "stat_thresh2",
      "name": "Stat thresh2",
      "type": "Number",
      "list": true,
      "min-list-entries": 2,
      "max-list-entries": 2,
      "value-key": "[STAT_THRESH2]",
      "description": "(a float, a float). Min and max values for second statistical overlay.",
      "optional": true
    },
    {
      "id": "use_checkerboard",
      "name": "Use checkerboard",
      "type": "Flag",
      "value-key": "[USE_CHECKERBOARD]",
      "command-line-flag": "-c",
      "description": "Use checkerboard mask for overlay.",
      "optional": true
    }
  ],
  "output-files": [
    {
      "name": "Out file",
      "id": "out_file_outfile",
      "path-template": "[OUT_FILE]",
      "optional": true,
      "description": "Combined image volume."
    }
  ],
  "groups": [
    {
      "id": "mutex_group",
      "name": "Mutex group",
      "members": [
        "bg_thresh",
        "full_bg_range",
        "auto_thresh_bg"
      ],
      "mutually-exclusive": true
    },
    {
      "id": "mutex_group_2",
      "name": "Mutex group 2",
      "members": [
        "stat_image2"
      ],
      "mutually-exclusive": true
    }
  ],
  "tool-version": "6.0.4",
  "schema-version": "0.5",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  },
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki"
}