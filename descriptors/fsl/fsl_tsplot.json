{
  "tool-version": "6.0.5",
  "name": "fsl_tsplot",
  "author": "University of Oxford (Christian F. Beckmann)",
  "description": "Timeseries plotting tool from FSL",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki/",
  "command-line": "fsl_tsplot [INPUT_FILES] [OUTPUT_FILE] [TITLE] [LEGEND_FILE] [LABELS] [YMIN] [YMAX] [XLABEL] [YLABEL] [HEIGHT] [WIDTH] [UNIT] [PRECISION] [SCI_FLAG] [START_COL] [END_COL]",
  "container-image": {
    "image": "mcin/fsl:6.0.5",
    "type": "docker"
  },
  "inputs": [
    {
      "command-line-flag": "-i",
      "description": "Comma-separated list of input file names (ASCII text matrix, one column per timecourse)",
      "value-key": "[INPUT_FILES]",
      "type": "String",
      "optional": false,
      "id": "input_files",
      "name": "Input Files"
    },
    {
      "command-line-flag": "-o",
      "description": "Output filename for the PNG file",
      "value-key": "[OUTPUT_FILE]",
      "type": "String",
      "optional": false,
      "id": "output_file",
      "name": "Output File"
    },
    {
      "command-line-flag": "-t",
      "description": "Plot title",
      "value-key": "[TITLE]",
      "type": "String",
      "optional": true,
      "id": "title",
      "name": "Plot Title"
    },
    {
      "command-line-flag": "-l",
      "description": "File name of ASCII text file, one row per legend entry",
      "value-key": "[LEGEND_FILE]",
      "type": "String",
      "optional": true,
      "id": "legend_file",
      "name": "Legend File"
    },
    {
      "command-line-flag": "-a",
      "description": "Comma-separated list of labels",
      "value-key": "[LABELS]",
      "type": "String",
      "optional": true,
      "id": "labels",
      "name": "Labels"
    },
    {
      "command-line-flag": "--ymin",
      "description": "Minimum y-value",
      "value-key": "[YMIN]",
      "type": "Number",
      "optional": true,
      "id": "ymin",
      "name": "Minimum Y-Value"
    },
    {
      "command-line-flag": "--ymax",
      "description": "Maximum y-value",
      "value-key": "[YMAX]",
      "type": "Number",
      "optional": true,
      "id": "ymax",
      "name": "Maximum Y-Value"
    },
    {
      "command-line-flag": "-x",
      "description": "X-axis label",
      "value-key": "[XLABEL]",
      "type": "String",
      "optional": true,
      "id": "xlabel",
      "name": "X-Axis Label"
    },
    {
      "command-line-flag": "-y",
      "description": "Y-axis label",
      "value-key": "[YLABEL]",
      "type": "String",
      "optional": true,
      "id": "ylabel",
      "name": "Y-Axis Label"
    },
    {
      "command-line-flag": "-h",
      "description": "Plot height in pixels (default 150)",
      "value-key": "[HEIGHT]",
      "type": "Number",
      "optional": true,
      "id": "height",
      "name": "Plot Height"
    },
    {
      "command-line-flag": "-w",
      "description": "Plot width in pixels (default 600)",
      "value-key": "[WIDTH]",
      "type": "Number",
      "optional": true,
      "id": "width",
      "name": "Plot Width"
    },
    {
      "command-line-flag": "-u",
      "description": "Scaling units for x-axis (default 1...length of infile)",
      "value-key": "[UNIT]",
      "type": "Number",
      "optional": true,
      "id": "unit",
      "name": "X-Axis Units"
    },
    {
      "command-line-flag": "--precision",
      "description": "Precision of x-axis labels",
      "value-key": "[PRECISION]",
      "type": "Number",
      "optional": true,
      "id": "precision",
      "name": "X-Axis Label Precision"
    },
    {
      "command-line-flag": "--sci",
      "description": "Switch on scientific notation",
      "value-key": "[SCI_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "sci_flag",
      "name": "Scientific Notation Flag"
    },
    {
      "command-line-flag": "--start",
      "description": "Position of first column to plot",
      "value-key": "[START_COL]",
      "type": "Number",
      "optional": true,
      "id": "start_col",
      "name": "Start Column"
    },
    {
      "command-line-flag": "--finish",
      "description": "Position of final column to plot",
      "value-key": "[END_COL]",
      "type": "Number",
      "optional": true,
      "id": "end_col",
      "name": "End Column"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "Output PNG file",
      "optional": false,
      "id": "output_png",
      "name": "Output PNG File"
    }
  ]
}
