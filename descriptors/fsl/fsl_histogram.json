{
  "tool-version": "6.0.4",
  "name": "fsl_histogram",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "Histogram plotting tool for FSL",
  "command-line": "fsl_histogram [INPUT_FILE_DUPLICATE] [OUTPUT_FILE_DUPLICATE] [MASK_FILE_DUPLICATE] [GMMFIT_FILE_DUPLICATE] [PLOT_TITLE_DUPLICATE] [LEGEND_FILE_DUPLICATE] [XLABEL_DUPLICATE] [YLABEL_DUPLICATE] [PLOT_HEIGHT_DUPLICATE] [PLOT_WIDTH_DUPLICATE] [NUM_BINS_DUPLICATE] [ZOOM_FACTOR_DUPLICATE] [USE_GMM_FLAG]",
  "inputs": [
    {
      "command-line-flag": "-i",
      "description": "Input file name",
      "value-key": "[INPUT_FILE]",
      "type": "File",
      "optional": false,
      "id": "input_file",
      "name": "Input File"
    },
    {
      "command-line-flag": "--in",
      "description": "Input file name",
      "value-key": "[INPUT_FILE_DUPLICATE]",
      "type": "File",
      "optional": false,
      "id": "input_file_duplicate",
      "name": "Input File (duplicate entry)"
    },
    {
      "command-line-flag": "-o",
      "description": "Output filename for the PNG file",
      "value-key": "[OUTPUT_FILE]",
      "type": "String",
      "optional": false,
      "id": "output_file",
      "name": "Output File"
    },
    {
      "command-line-flag": "--out",
      "description": "Output filename for the PNG file",
      "value-key": "[OUTPUT_FILE_DUPLICATE]",
      "type": "String",
      "optional": false,
      "id": "output_file_duplicate",
      "name": "Output File (duplicate entry)"
    },
    {
      "command-line-flag": "-m",
      "description": "Mask file name",
      "value-key": "[MASK_FILE]",
      "type": "File",
      "optional": true,
      "id": "mask_file",
      "name": "Mask File"
    },
    {
      "command-line-flag": "--mask",
      "description": "Mask file name",
      "value-key": "[MASK_FILE_DUPLICATE]",
      "type": "File",
      "optional": true,
      "id": "mask_file_duplicate",
      "name": "Mask File (duplicate entry)"
    },
    {
      "command-line-flag": "-f",
      "description": "File name of matrix with parameter estimates of Gaussian/Gamma mixture model (means, variances and proportions per row)",
      "value-key": "[GMMFIT_FILE]",
      "type": "File",
      "optional": true,
      "id": "gmmfit_file",
      "name": "GMM Fit File"
    },
    {
      "command-line-flag": "--gmmfit",
      "description": "File name of matrix with parameter estimates of Gaussian/Gamma mixture model (means, variances and proportions per row)",
      "value-key": "[GMMFIT_FILE_DUPLICATE]",
      "type": "File",
      "optional": true,
      "id": "gmmfit_file_duplicate",
      "name": "GMM Fit File (duplicate entry)"
    },
    {
      "command-line-flag": "-t",
      "description": "Plot title",
      "value-key": "[PLOT_TITLE]",
      "type": "String",
      "optional": true,
      "id": "plot_title",
      "name": "Plot Title"
    },
    {
      "command-line-flag": "--title",
      "description": "Plot title",
      "value-key": "[PLOT_TITLE_DUPLICATE]",
      "type": "String",
      "optional": true,
      "id": "plot_title_duplicate",
      "name": "Plot Title (duplicate entry)"
    },
    {
      "command-line-flag": "-l",
      "description": "File name of ASCII text file, one row per legend entry",
      "value-key": "[LEGEND_FILE]",
      "type": "File",
      "optional": true,
      "id": "legend_file",
      "name": "Legend File"
    },
    {
      "command-line-flag": "--legend",
      "description": "File name of ASCII text file, one row per legend entry",
      "value-key": "[LEGEND_FILE_DUPLICATE]",
      "type": "File",
      "optional": true,
      "id": "legend_file_duplicate",
      "name": "Legend File (duplicate entry)"
    },
    {
      "command-line-flag": "-x",
      "description": "X-axis label",
      "value-key": "[XLABEL]",
      "type": "String",
      "optional": true,
      "id": "xlabel",
      "name": "X-axis Label"
    },
    {
      "command-line-flag": "--xlabel",
      "description": "X-axis label",
      "value-key": "[XLABEL_DUPLICATE]",
      "type": "String",
      "optional": true,
      "id": "xlabel_duplicate",
      "name": "X-axis Label (duplicate entry)"
    },
    {
      "command-line-flag": "-y",
      "description": "Y-axis label",
      "value-key": "[YLABEL]",
      "type": "String",
      "optional": true,
      "id": "ylabel",
      "name": "Y-axis Label"
    },
    {
      "command-line-flag": "--ylabel",
      "description": "Y-axis label",
      "value-key": "[YLABEL_DUPLICATE]",
      "type": "String",
      "optional": true,
      "id": "ylabel_duplicate",
      "name": "Y-axis Label (duplicate entry)"
    },
    {
      "command-line-flag": "-h",
      "description": "Plot height in pixels (default 400)",
      "value-key": "[PLOT_HEIGHT]",
      "type": "Number",
      "optional": true,
      "id": "plot_height",
      "name": "Plot Height"
    },
    {
      "command-line-flag": "--height",
      "description": "Plot height in pixels (default 400)",
      "value-key": "[PLOT_HEIGHT_DUPLICATE]",
      "type": "Number",
      "optional": true,
      "id": "plot_height_duplicate",
      "name": "Plot Height (duplicate entry)"
    },
    {
      "command-line-flag": "-w",
      "description": "Plot width in pixels (default 600)",
      "value-key": "[PLOT_WIDTH]",
      "type": "Number",
      "optional": true,
      "id": "plot_width",
      "name": "Plot Width"
    },
    {
      "command-line-flag": "--width",
      "description": "Plot width in pixels (default 600)",
      "value-key": "[PLOT_WIDTH_DUPLICATE]",
      "type": "Number",
      "optional": true,
      "id": "plot_width_duplicate",
      "name": "Plot Width (duplicate entry)"
    },
    {
      "command-line-flag": "-b",
      "description": "Number of histogram bins",
      "value-key": "[NUM_BINS]",
      "type": "Number",
      "optional": true,
      "id": "num_bins",
      "name": "Number of Bins"
    },
    {
      "command-line-flag": "--bins",
      "description": "Number of histogram bins",
      "value-key": "[NUM_BINS_DUPLICATE]",
      "type": "Number",
      "optional": true,
      "id": "num_bins_duplicate",
      "name": "Number of Bins (duplicate entry)"
    },
    {
      "command-line-flag": "-d",
      "description": "Zoom factor for y-range (e.g. 2.0)",
      "value-key": "[ZOOM_FACTOR]",
      "type": "Number",
      "optional": true,
      "id": "zoom_factor",
      "name": "Zoom Factor"
    },
    {
      "command-line-flag": "--detail",
      "description": "Zoom factor for y-range (e.g. 2.0)",
      "value-key": "[ZOOM_FACTOR_DUPLICATE]",
      "type": "Number",
      "optional": true,
      "id": "zoom_factor_duplicate",
      "name": "Zoom Factor (duplicate entry)"
    },
    {
      "command-line-flag": "--gmm",
      "description": "Use Gaussian mixture model instead of Gaussian/Gamma mixture model",
      "value-key": "[USE_GMM_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "use_gmm_flag",
      "name": "Use Gaussian Mixture Model"
    }
  ],
  "schema-version": "0.5",
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "Generated PNG file",
      "optional": true,
      "id": "png_file",
      "name": "PNG Output File"
    }
  ],
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  }
}
