{
  "tool-version": "6.0.5",
  "name": "fsl_get_standard",
  "description": "Generate paths to FSL standard space images",
  "command-line": "fsl_get_standard [IMAGE_TYPE] [RESOLUTION] [VERBOSE_FLAG] [VERSION_FLAG]",
  "inputs": [
    {
      "description": "Image type - one of 'whole_head' (the default), 'brain', or 'brain_mask'",
      "value-key": "[IMAGE_TYPE]",
      "value-choices": ["whole_head", "brain", "brain_mask"],
      "type": "String",
      "optional": false,
      "id": "image_type",
      "name": "Image type"
    },
    {
      "command-line-flag": "-r",
      "description": "Desired isotropic resolution in millimetres",
      "value-key": "[RESOLUTION]",
      "type": "Number",
      "optional": true,
      "id": "resolution",
      "name": "Resolution"
    },
    {
      "command-line-flag": "-v",
      "description": "Output more information",
      "value-key": "[VERBOSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "verbose_flag",
      "name": "Verbose"
    },
    {
      "command-line-flag": "-V",
      "description": "Print version and exit",
      "value-key": "[VERSION_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "version_flag",
      "name": "Version"
    }
  ],
  "schema-version": "0.5"
}
