{
  "tool-version": "6.0.4",
  "name": "swap_subjectwise",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "Reordering of the dyadic vectors and fsamples according to average inter-subject modal orientations",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki",
  "command-line": "swap_subjectwise [DYADS] [FMEAN] [MASK] [OBASENAME] [XTHRESH] [AVERAGEONLY_FLAG] [VERBOSE_FLAG]",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  },
  "inputs": [
    {
      "command-line-flag": "-r",
      "description": "List of list of dyads",
      "value-key": "[DYADS]",
      "type": "File",
      "optional": false,
      "id": "dyads",
      "name": "List of dyads"
    },
    {
      "command-line-flag": "-f",
      "description": "List of list of mean fsamples",
      "value-key": "[FMEAN]",
      "type": "File",
      "optional": false,
      "id": "fmean",
      "name": "List of mean fsamples"
    },
    {
      "command-line-flag": "-m",
      "description": "Filename of brain mask",
      "value-key": "[MASK]",
      "type": "File",
      "optional": true,
      "id": "mask",
      "name": "Brain mask"
    },
    {
      "command-line-flag": "-b",
      "description": "Output obasename [default=swapped]",
      "value-key": "[OBASENAME]",
      "type": "String",
      "optional": true,
      "id": "obasename",
      "name": "Output obasename"
    },
    {
      "command-line-flag": "--xthresh",
      "description": "A.R.D. threshold - default=0.1",
      "value-key": "[XTHRESH]",
      "type": "Number",
      "integer": false,
      "optional": true,
      "id": "xthresh",
      "name": "A.R.D. threshold"
    },
    {
      "command-line-flag": "--averageonly",
      "description": "Average only?",
      "value-key": "[AVERAGEONLY_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "averageonly_flag",
      "name": "Average only flag"
    },
    {
      "command-line-flag": "--verbose",
      "description": "Switch on diagnostic messages",
      "value-key": "[VERBOSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "verbose_flag",
      "name": "Verbose Flag"
    }
  ],
  "schema-version": "0.5",
  "groups": [
    {
      "description": "Compulsory arguments",
      "id": "compulsory_params_group",
      "members": ["dyads", "fmean", "mask"],
      "name": "Compulsory Parameters"
    },
    {
      "description": "Optional arguments",
      "id": "optional_params_group",
      "members": ["obasename", "xthresh", "averageonly_flag", "verbose_flag"],
      "name": "Optional Parameters"
    }
  ]
}
