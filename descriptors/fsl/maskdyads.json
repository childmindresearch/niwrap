{
  "tool-version": "6.0.5",
  "name": "maskdyads",
  "author": "Author Name",
  "description": "Tool to mask dyads with threshold",
  "command-line": "maskdyads [DYADS] [FSAMPLES] [THR]",
  "inputs": [
    {
      "description": "Input dyads file",
      "value-key": "[DYADS]",
      "type": "File",
      "optional": false,
      "id": "dyads",
      "name": "Dyads file"
    },
    {
      "description": "Input fsamples file",
      "value-key": "[FSAMPLES]",
      "type": "File",
      "optional": false,
      "id": "fsamples",
      "name": "Fsamples file"
    },
    {
      "command-line-flag": "[THR]",
      "description": "Threshold (default is 0.05)",
      "value-key": "[THR]",
      "type": "Number",
      "default-value": 0.05,
      "optional": true,
      "id": "threshold",
      "name": "Threshold"
    }
  ],
  "schema-version": "0.5"
}
