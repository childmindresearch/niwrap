{
  "tool-version": "6.0.4",
  "name": "dtifit",
  "author": "FMRIB Analysis Group, University of Oxford",
  "descriptor-url": "N/A",
  "description": "DTIFIT - Fit a diffusion tensor model at each voxel",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki",
  "command-line": "dtifit [DATA_FILE] [OUTPUT_BASENAME] [MASK_FILE] [BVEC_FILE] [BVAL_FILE] [VERBOSE_FLAG] [SSE_FLAG] [WLS_FLAG] [KURT_FLAG] [KURTDIR_FLAG] [LITTLEBIT_FLAG] [SAVE_TENSOR_FLAG] [ZMIN] [ZMAX] [YMIN] [YMAX] [XMIN] [XMAX] [GRADNONLIN_FILE] [CONFOUND_REGRESSORS]",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  },
  "inputs": [
    {
      "command-line-flag": "-k",
      "description": "DTI data file",
      "value-key": "[DATA_FILE]",
      "type": "File",
      "optional": false,
      "id": "data_file",
      "name": "Data file"
    },
    {
      "command-line-flag": "-o",
      "description": "Output basename",
      "value-key": "[OUTPUT_BASENAME]",
      "type": "String",
      "optional": false,
      "id": "output_basename",
      "name": "Output basename"
    },
    {
      "command-line-flag": "-m",
      "description": "Bet binary mask file",
      "value-key": "[MASK_FILE]",
      "type": "File",
      "optional": false,
      "id": "mask_file",
      "name": "Mask file"
    },
    {
      "command-line-flag": "-r",
      "description": "B vectors file",
      "value-key": "[BVEC_FILE]",
      "type": "File",
      "optional": false,
      "id": "bvec_file",
      "name": "B vectors file"
    },
    {
      "command-line-flag": "-b",
      "description": "B values file",
      "value-key": "[BVAL_FILE]",
      "type": "File",
      "optional": false,
      "id": "bval_file",
      "name": "B values file"
    },
    {
      "command-line-flag": "-V",
      "description": "Switch on diagnostic messages",
      "value-key": "[VERBOSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "verbose_flag",
      "name": "Verbose flag"
    },
    {
      "command-line-flag": "--sse",
      "description": "Output sum of squared errors",
      "value-key": "[SSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "sse_flag",
      "name": "SSE flag"
    },
    {
      "command-line-flag": "-w",
      "description": "Fit the tensor with weighted least squares",
      "value-key": "[WLS_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "wls_flag",
      "name": "WLS flag"
    },
    {
      "command-line-flag": "--kurt",
      "description": "Output mean kurtosis map (for multi-shell data)",
      "value-key": "[KURT_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "kurt_flag",
      "name": "Kurt flag"
    },
    {
      "command-line-flag": "--kurtdir",
      "description": "Output maps of kurtosis along each eigenvector: K1, K2, and K3 (for multi-shell data)",
      "value-key": "[KURTDIR_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "kurtdir_flag",
      "name": "Kurt direction flag"
    },
    {
      "command-line-flag": "--littlebit",
      "description": "Only process small area of brain",
      "value-key": "[LITTLEBIT_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "littlebit_flag",
      "name": "Little bit flag"
    },
    {
      "command-line-flag": "--save_tensor",
      "description": "Save the elements of the tensor",
      "value-key": "[SAVE_TENSOR_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "save_tensor_flag",
      "name": "Save tensor flag"
    },
    {
      "command-line-flag": "-z",
      "description": "Minimum z",
      "value-key": "[ZMIN]",
      "type": "Number",
      "optional": true,
      "id": "zmin",
      "name": "Minimum z"
    },
    {
      "command-line-flag": "-Z",
      "description": "Maximum z",
      "value-key": "[ZMAX]",
      "type": "Number",
      "optional": true,
      "id": "zmax",
      "name": "Maximum z"
    },
    {
      "command-line-flag": "-y",
      "description": "Minimum y",
      "value-key": "[YMIN]",
      "type": "Number",
      "optional": true,
      "id": "ymin",
      "name": "Minimum y"
    },
    {
      "command-line-flag": "-Y",
      "description": "Maximum y",
      "value-key": "[YMAX]",
      "type": "Number",
      "optional": true,
      "id": "ymax",
      "name": "Maximum y"
    },
    {
      "command-line-flag": "-x",
      "description": "Minimum x",
      "value-key": "[XMIN]",
      "type": "Number",
      "optional": true,
      "id": "xmin",
      "name": "Minimum x"
    },
    {
      "command-line-flag": "-X",
      "description": "Maximum x",
      "value-key": "[XMAX]",
      "type": "Number",
      "optional": true,
      "id": "xmax",
      "name": "Maximum x"
    },
    {
      "command-line-flag": "--gradnonlin",
      "description": "Gradient Nonlinearity Tensor file",
      "value-key": "[GRADNONLIN_FILE]",
      "type": "File",
      "optional": true,
      "id": "gradnonlin_file",
      "name": "Gradient nonlinearity tensor file"
    },
    {
      "command-line-flag": "--cni",
      "description": "Input confound regressors",
      "value-key": "[CONFOUND_REGRESSORS]",
      "type": "File",
      "optional": true,
      "id": "confound_regressors",
      "name": "Confound regressors file"
    }
  ],
  "schema-version": "0.5",
  "groups": [
    {
      "description": "Compulsory arguments",
      "id": "mandatory_args_group",
      "members": [
        "data_file",
        "output_basename",
        "mask_file",
        "bvec_file",
        "bval_file"
      ],
      "name": "Mandatory Arguments"
    },
    {
      "description": "Optional arguments",
      "id": "optional_args_group",
      "members": [
        "verbose_flag",
        "sse_flag",
        "wls_flag",
        "kurt_flag",
        "kurtdir_flag",
        "littlebit_flag",
        "save_tensor_flag",
        "zmin",
        "zmax",
        "ymin",
        "ymax",
        "xmin",
        "xmax",
        "gradnonlin_file",
        "confound_regressors"
      ],
      "name": "Optional Arguments"
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_BASENAME]_FA.nii.gz",
      "description": "Fractional anisotropy output",
      "optional": true,
      "id": "fa_output",
      "name": "FA output"
    },
    {
      "path-template": "[OUTPUT_BASENAME]_MD.nii.gz",
      "description": "Mean diffusivity output",
      "optional": true,
      "id": "md_output",
      "name": "MD output"
    },
    {
      "path-template": "[OUTPUT_BASENAME]_MO.nii.gz",
      "description": "Mode of the anisotropy output",
      "optional": true,
      "id": "mo_output",
      "name": "MO output"
    },
    {
      "path-template": "[OUTPUT_BASENAME]_L1.nii.gz",
      "description": "1st eigenvalue output",
      "optional": true,
      "id": "l1_output",
      "name": "L1 output"
    },
    {
      "path-template": "[OUTPUT_BASENAME]_L2.nii.gz",
      "description": "2nd eigenvalue output",
      "optional": true,
      "id": "l2_output",
      "name": "L2 output"
    },
    {
      "path-template": "[OUTPUT_BASENAME]_L3.nii.gz",
      "description": "3rd eigenvalue output",
      "optional": true,
      "id": "l3_output",
      "name": "L3 output"
    }
  ]
}
