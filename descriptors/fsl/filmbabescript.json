{
  "tool-version": "6.0.4",
  "name": "filmbabescript",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "A tool/script for processing FEAT directories and FLOBs directories",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki",
  "command-line": "filmbabescript [FEAT_DIR] [FLOBS_DIR]",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  },
  "inputs": [
    {
      "description": "Input FEAT directory",
      "value-key": "[FEAT_DIR]",
      "type": "String",
      "optional": false,
      "id": "feat_dir",
      "name": "FEAT Directory"
    },
    {
      "description": "Input FLOBs directory",
      "value-key": "[FLOBS_DIR]",
      "type": "String",
      "optional": false,
      "id": "flobs_dir",
      "name": "FLOBs Directory"
    }
  ],
  "schema-version": "0.5"
}
