{
  "tool-version": "6.0.5",
  "name": "fslvbm_2_template",
  "author": "FMRIB Analysis Group, University of Oxford",
  "descriptor-url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki/FSLVBM",
  "description": "FSL-VBM is a Voxel-Based Morphometry style analysis tool for FSL.",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki",
  "command-line": "fslvbm_2_template [ARCH] [COPROCESSOR] [COPROCESSOR_MULTI] [COPROCESSOR_CLASS] [COPROCESSOR_TOOLKIT] [JOBHOLD] [ARRAY_HOLD] [LOGDIR] [MAILOPTIONS] [MAILTO] [NAME] [PRIORITY] [QUEUE] [RESOURCE] [DELETE_JOB] [MEMORY_GB] [PARALLEL_ENV] [ARRAY_TASK] [ARRAY_NATIVE] [NUM_TASKS] [COPROCESSOR_NAME] [PROJECT] [RUNTIME_LIMIT] [JOB_FILE]",
  "container-image": {
    "image": "mcin/fsl:6.0.5",
    "type": "docker"
  },
  "inputs": [
    {
      "command-line-flag": "-a",
      "description": "Specify architecture",
      "value-key": "[ARCH]",
      "type": "String",
      "optional": true,
      "id": "arch",
      "name": "Architecture"
    },
    {
      "command-line-flag": "-c",
      "description": "Specify coprocessor type",
      "value-key": "[COPROCESSOR]",
      "type": "String",
      "optional": true,
      "id": "coprocessor",
      "name": "Coprocessor"
    },
    {
      "command-line-flag": "--coprocessor_multi",
      "description": "Specify multiple coprocessors",
      "value-key": "[COPROCESSOR_MULTI]",
      "type": "String",
      "optional": true,
      "id": "coprocessor_multi",
      "name": "Coprocessor Multi"
    },
    {
      "command-line-flag": "--coprocessor_class",
      "description": "Specify coprocessor class",
      "value-key": "[COPROCESSOR_CLASS]",
      "type": "String",
      "optional": true,
      "id": "coprocessor_class",
      "name": "Coprocessor Class"
    },
    {
      "command-line-flag": "--coprocessor_toolkit",
      "description": "Specify coprocessor toolkit",
      "value-key": "[COPROCESSOR_TOOLKIT]",
      "type": "String",
      "optional": true,
      "id": "coprocessor_toolkit",
      "name": "Coprocessor Toolkit"
    },
    {
      "command-line-flag": "-j",
      "description": "Hold job until specified job ID is completed",
      "value-key": "[JOBHOLD]",
      "type": "String",
      "optional": true,
      "id": "jobhold",
      "name": "Job Hold"
    },
    {
      "command-line-flag": "--array_hold",
      "description": "Hold array job until specified job ID is completed",
      "value-key": "[ARRAY_HOLD]",
      "type": "String",
      "optional": true,
      "id": "array_hold",
      "name": "Array Hold"
    },
    {
      "command-line-flag": "-l",
      "description": "Specify log directory",
      "value-key": "[LOGDIR]",
      "type": "String",
      "optional": true,
      "id": "logdir",
      "name": "Log Directory"
    },
    {
      "command-line-flag": "-m",
      "description": "Specify mail options",
      "value-key": "[MAILOPTIONS]",
      "type": "String",
      "optional": true,
      "id": "mailoptions",
      "name": "Mail Options"
    },
    {
      "command-line-flag": "-M",
      "description": "Specify email address for notifications",
      "value-key": "[MAILTO]",
      "type": "String",
      "optional": true,
      "id": "mailto",
      "name": "Mail To"
    },
    {
      "command-line-flag": "-N",
      "description": "Specify job name",
      "value-key": "[NAME]",
      "type": "String",
      "optional": true,
      "id": "name",
      "name": "Job Name"
    },
    {
      "command-line-flag": "-p",
      "description": "Specify job priority",
      "value-key": "[PRIORITY]",
      "type": "String",
      "optional": true,
      "id": "priority",
      "name": "Priority"
    },
    {
      "command-line-flag": "-q",
      "description": "Specify queue",
      "value-key": "[QUEUE]",
      "type": "String",
      "optional": true,
      "id": "queue",
      "name": "Queue"
    },
    {
      "command-line-flag": "-r",
      "description": "Specify resources",
      "value-key": "[RESOURCE]",
      "type": "String",
      "optional": true,
      "id": "resource",
      "name": "Resource"
    },
    {
      "command-line-flag": "--delete_job",
      "description": "Delete specified job",
      "value-key": "[DELETE_JOB]",
      "type": "String",
      "optional": true,
      "id": "delete_job",
      "name": "Delete Job"
    },
    {
      "command-line-flag": "-R",
      "description": "Specify memory in GB",
      "value-key": "[MEMORY_GB]",
      "type": "Number",
      "optional": true,
      "id": "memory_gb",
      "name": "Memory (GB)"
    },
    {
      "command-line-flag": "-s",
      "description": "Specify parallel environment and threads",
      "value-key": "[PARALLEL_ENV]",
      "type": "String",
      "optional": true,
      "id": "parallel_env",
      "name": "Parallel Environment"
    },
    {
      "command-line-flag": "-t",
      "description": "Specify array task",
      "value-key": "[ARRAY_TASK]",
      "type": "String",
      "optional": true,
      "id": "array_task",
      "name": "Array Task"
    },
    {
      "command-line-flag": "--array_native",
      "description": "Specify array native task",
      "value-key": "[ARRAY_NATIVE]",
      "type": "String",
      "optional": true,
      "id": "array_native",
      "name": "Array Native"
    },
    {
      "command-line-flag": "-x",
      "description": "Specify number of tasks",
      "value-key": "[NUM_TASKS]",
      "type": "Number",
      "optional": true,
      "id": "num_tasks",
      "name": "Number of Tasks"
    },
    {
      "command-line-flag": "--has_coprocessor",
      "description": "Specify coprocessor name",
      "value-key": "[COPROCESSOR_NAME]",
      "type": "String",
      "optional": true,
      "id": "coprocessor_name",
      "name": "Coprocessor Name"
    },
    {
      "command-line-flag": "--project",
      "description": "Specify project",
      "value-key": "[PROJECT]",
      "type": "String",
      "optional": true,
      "id": "project",
      "name": "Project"
    },
    {
      "command-line-flag": "-T",
      "description": "Specify runtime limit in minutes",
      "value-key": "[RUNTIME_LIMIT]",
      "type": "Number",
      "optional": true,
      "id": "runtime_limit",
      "name": "Runtime Limit"
    },
    {
      "command-line-flag": "-z",
      "description": "Specify job script file",
      "value-key": "[JOB_FILE]",
      "type": "File",
      "optional": true,
      "id": "job_file",
      "name": "Job Script File"
    }
  ],
  "schema-version": "0.5"
}