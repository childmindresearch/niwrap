{
  "tool-version": "6.0.4",
  "name": "run_first",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "A tool to run FSL's FIRST for subcortical segmentation",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki",
  "command-line": "run_first -i [INPUT_IMAGE] -t [TRANSFORMATION_MATRIX] -n [N_MODES] -o [OUTPUT_BASENAME] -m [MODEL_NAME] [VERBOSE] [INTREF] [LOADBVARS] [MULTIPLE_IMAGES]",
  "container-image": {
    "type": "docker",
    "image": "brainlife/fsl:6.0.4-patched2"
  },
  "inputs": [
    {
      "description": "Input image file (e.g. img.nii.gz)",
      "value-key": "[INPUT_IMAGE]",
      "type": "File",
      "optional": false,
      "id": "input_image",
      "name": "Input Image"
    },
    {
      "description": "Input transformation matrix file (e.g. input_to_mni.mat)",
      "value-key": "[TRANSFORMATION_MATRIX]",
      "type": "File",
      "optional": false,
      "id": "transformation_matrix",
      "name": "Transformation Matrix"
    },
    {
      "description": "Number of modes",
      "value-key": "[N_MODES]",
      "type": "Number",
      "optional": false,
      "id": "n_modes",
      "name": "Number of Modes"
    },
    {
      "description": "Output basename",
      "value-key": "[OUTPUT_BASENAME]",
      "type": "String",
      "optional": false,
      "id": "output_basename",
      "name": "Output Basename"
    },
    {
      "description": "Model name",
      "value-key": "[MODEL_NAME]",
      "type": "File",
      "optional": false,
      "id": "model_name",
      "name": "Model Name"
    },
    {
      "command-line-flag": "-v",
      "description": "Verbose mode",
      "value-key": "[VERBOSE]",
      "type": "Flag",
      "optional": true,
      "id": "verbose_flag",
      "name": "Verbose Flag"
    },
    {
      "command-line-flag": "-intref",
      "description": "Reference structure for the local intensity normalization",
      "value-key": "[INTREF]",
      "type": "String",
      "optional": true,
      "id": "intref_model_name",
      "name": "Intensity Reference Model Name"
    },
    {
      "command-line-flag": "-loadBvars",
      "description": "Initializes FIRST with a previous estimate of the structure",
      "value-key": "[LOADBVARS]",
      "type": "File",
      "optional": true,
      "id": "load_bvars",
      "name": "Load Bvars"
    },
    {
      "command-line-flag": "-multipleImages",
      "description": "Run FIRST on multiple images; provide a list of images, transformation matrices, and output names",
      "value-key": "[MULTIPLE_IMAGES]",
      "type": "Flag",
      "optional": true,
      "id": "multiple_images_flag",
      "name": "Multiple Images Flag"
    }
  ],
  "schema-version": "0.5",
  "groups": [
    {
      "description": "Optional parameters for running FIRST",
      "id": "optional_parameters_group",
      "members": [
        "verbose_flag",
        "intref_model_name",
        "load_bvars",
        "multiple_images_flag"
      ],
      "name": "Optional Parameters"
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_BASENAME]*",
      "description": "Output files generated by FIRST",
      "optional": true,
      "id": "output_files",
      "name": "Output Files"
    }
  ]
}
