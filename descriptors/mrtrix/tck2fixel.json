{
  "name": "tck2fixel",
  "description": "Compute a fixel TDI map from a tractogram.\n\n\n\nReferences:\n\n",
  "author": "David Raffelt (david.raffelt@florey.edu.au)",
  "tool-version": "3.0.4",
  "schema-version": "0.5",
  "container-image": {
    "image": "mrtrix3/mrtrix3:3.0.4",
    "type": "docker"
  },
  "command-line": "tck2fixel [OPTIONS_ANGLE] [__STANDARD_OPTIONS_INFO] [__STANDARD_OPTIONS_QUIET] [__STANDARD_OPTIONS_DEBUG] [__STANDARD_OPTIONS_FORCE] [__STANDARD_OPTIONS_NTHREADS] [__STANDARD_OPTIONS_CONFIG] [__STANDARD_OPTIONS_HELP] [__STANDARD_OPTIONS_VERSION] [TRACKS] [FIXEL_FOLDER_IN] [FIXEL_FOLDER_OUT] [FIXEL_DATA_OUT]",
  "inputs": [
    {
      "id": "tracks",
      "name": "tracks",
      "command-line-flag": "",
      "value-key": "[TRACKS]",
      "description": "the input tracks.",
      "type": "File",
      "optional": false
    },
    {
      "id": "fixel_folder_in",
      "name": "fixel_folder_in",
      "command-line-flag": "",
      "value-key": "[FIXEL_FOLDER_IN]",
      "description": "the input fixel folder. Used to define the fixels and their directions",
      "type": "File",
      "optional": false
    },
    {
      "id": "fixel_folder_out",
      "name": "fixel_folder_out",
      "command-line-flag": "",
      "value-key": "[FIXEL_FOLDER_OUT]",
      "description": "the fixel folder to which the output will be written. This can be the same as the input folder if desired",
      "type": "String",
      "optional": false
    },
    {
      "id": "fixel_data_out",
      "name": "fixel_data_out",
      "command-line-flag": "",
      "value-key": "[FIXEL_DATA_OUT]",
      "description": "the name of the fixel data image.",
      "type": "String",
      "optional": false
    },
    {
      "id": "angle",
      "name": "angle",
      "command-line-flag": "-angle",
      "value-key": "[OPTIONS_ANGLE]",
      "description": "the max angle threshold for assigning streamline tangents to fixels (Default: 45 degrees)",
      "type": "Number",
      "optional": true,
      "integer": false
    },
    {
      "id": "info",
      "name": "info",
      "command-line-flag": "-info",
      "value-key": "[__STANDARD_OPTIONS_INFO]",
      "description": "display information messages.",
      "type": "Flag",
      "optional": true
    },
    {
      "id": "quiet",
      "name": "quiet",
      "command-line-flag": "-quiet",
      "value-key": "[__STANDARD_OPTIONS_QUIET]",
      "description": "do not display information messages or progress status; alternatively, this can be achieved by setting the MRTRIX_QUIET environment variable to a non-empty string.",
      "type": "Flag",
      "optional": true
    },
    {
      "id": "debug",
      "name": "debug",
      "command-line-flag": "-debug",
      "value-key": "[__STANDARD_OPTIONS_DEBUG]",
      "description": "display debugging messages.",
      "type": "Flag",
      "optional": true
    },
    {
      "id": "force",
      "name": "force",
      "command-line-flag": "-force",
      "value-key": "[__STANDARD_OPTIONS_FORCE]",
      "description": "force overwrite of output files (caution: using the same file as input and output might cause unexpected behaviour).",
      "type": "Flag",
      "optional": true
    },
    {
      "id": "nthreads",
      "name": "nthreads",
      "command-line-flag": "-nthreads",
      "value-key": "[__STANDARD_OPTIONS_NTHREADS]",
      "description": "use this number of threads in multi-threaded applications (set to 0 to disable multi-threading).",
      "type": "Number",
      "optional": true,
      "integer": true
    },
    {
      "id": "config",
      "name": "config",
      "command-line-flag": "-config",
      "value-key": "[__STANDARD_OPTIONS_CONFIG]",
      "description": "temporarily set the value of an MRtrix config file entry.",
      "type": {
        "id": "config",
        "name": "config",
        "description": "temporarily set the value of an MRtrix config file entry.",
        "command-line": "-config [KEY] [VALUE]",
        "inputs": [
          {
            "id": "key",
            "name": "key",
            "command-line-flag": "",
            "value-key": "[KEY]",
            "description": "temporarily set the value of an MRtrix config file entry.",
            "type": "String",
            "optional": false
          },
          {
            "id": "value",
            "name": "value",
            "command-line-flag": "",
            "value-key": "[VALUE]",
            "description": "temporarily set the value of an MRtrix config file entry.",
            "type": "String",
            "optional": false
          }
        ],
        "output-files": []
      },
      "optional": true,
      "list": true
    },
    {
      "id": "help",
      "name": "help",
      "command-line-flag": "-help",
      "value-key": "[__STANDARD_OPTIONS_HELP]",
      "description": "display this information page and exit.",
      "type": "Flag",
      "optional": true
    },
    {
      "id": "version",
      "name": "version",
      "command-line-flag": "-version",
      "value-key": "[__STANDARD_OPTIONS_VERSION]",
      "description": "display version information and exit.",
      "type": "Flag",
      "optional": true
    }
  ],
  "output-files": [],
  "url": "https://mrtrix.readthedocs.io/en/latest/reference/commands/tck2fixel.html"
}